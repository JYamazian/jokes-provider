{
    "swagger": "2.0",
    "info": {
        "description": "A high-performance REST API service for serving random jokes with built-in caching, rate limiting, and health checks.",
        "title": "Jokes Provider API",
        "termsOfService": "http://swagger.io/terms/",
        "contact": {
            "name": "Jean Yamazian",
            "url": "https://jean.yamazian.com",
            "email": "jeanyamazian@outlook.com"
        },
        "license": {
            "name": "Apache 2.0",
            "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
        },
        "version": "1.0"
    },
    "host": "localhost:8080",
    "basePath": "/",
    "paths": {
        "/api/v1/metadata": {
            "get": {
                "description": "Returns comprehensive application metadata including version, configuration, and environment information",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "metadata"
                ],
                "summary": "Get application metadata",
                "responses": {
                    "200": {
                        "description": "Application metadata",
                        "schema": {
                            "$ref": "#/definitions/models.Metadata"
                        }
                    }
                }
            }
        },
        "/health/liveness": {
            "get": {
                "description": "Simple liveness probe to check if the service is running (uses Fiber built-in)",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "health"
                ],
                "summary": "Liveness probe",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/health/readiness": {
            "get": {
                "description": "Checks if the service is ready and dependencies are available (Redis, CSV file)",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "health"
                ],
                "summary": "Readiness check",
                "responses": {
                    "200": {
                        "description": "Service is ready",
                        "schema": {
                            "$ref": "#/definitions/models.ReadinessHealthStatus"
                        }
                    },
                    "503": {
                        "description": "Service is not ready",
                        "schema": {
                            "$ref": "#/definitions/models.ReadinessHealthStatus"
                        }
                    }
                }
            }
        },
        "/v1/jokes/random": {
            "get": {
                "description": "Returns a random joke from the jokes database. Supports caching.",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "jokes"
                ],
                "summary": "Get a random joke",
                "responses": {
                    "200": {
                        "description": "Random joke object with id and joke fields",
                        "schema": {
                            "$ref": "#/definitions/models.Joke"
                        }
                    },
                    "500": {
                        "description": "Failed to retrieve joke",
                        "schema": {
                            "type": "object",
                            "additionalProperties": {
                                "type": "string"
                            }
                        }
                    }
                }
            }
        },
        "/v1/jokes/{id}": {
            "get": {
                "description": "Returns a specific joke by its ID from the jokes database. Supports caching.",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "jokes"
                ],
                "summary": "Get a joke by ID",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Joke ID",
                        "name": "id",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Joke object with id and joke fields",
                        "schema": {
                            "$ref": "#/definitions/models.Joke"
                        }
                    },
                    "400": {
                        "description": "Joke ID is required",
                        "schema": {
                            "type": "object",
                            "additionalProperties": {
                                "type": "string"
                            }
                        }
                    },
                    "404": {
                        "description": "Joke not found",
                        "schema": {
                            "type": "object",
                            "additionalProperties": {
                                "type": "string"
                            }
                        }
                    },
                    "500": {
                        "description": "Failed to retrieve joke",
                        "schema": {
                            "type": "object",
                            "additionalProperties": {
                                "type": "string"
                            }
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "models.AppInfo": {
            "type": "object",
            "properties": {
                "flavor": {
                    "type": "string"
                },
                "name": {
                    "type": "string"
                },
                "version": {
                    "type": "string"
                }
            }
        },
        "models.CacheInfo": {
            "type": "object",
            "properties": {
                "enabled": {
                    "type": "boolean"
                },
                "ttl": {
                    "type": "string"
                },
                "url": {
                    "type": "string"
                }
            }
        },
        "models.FiberInfo": {
            "type": "object",
            "properties": {
                "case_sensitive": {
                    "type": "boolean"
                },
                "prefork": {
                    "type": "boolean"
                },
                "strict_routing": {
                    "type": "boolean"
                }
            }
        },
        "models.FilesInfo": {
            "type": "object",
            "properties": {
                "jokes_path": {
                    "type": "string"
                }
            }
        },
        "models.HeadersInfo": {
            "type": "object",
            "properties": {
                "country_header_name": {
                    "type": "string"
                },
                "ip_header_name": {
                    "type": "string"
                }
            }
        },
        "models.Joke": {
            "type": "object",
            "properties": {
                "id": {
                    "type": "string"
                },
                "joke": {
                    "type": "string"
                }
            }
        },
        "models.LoggingInfo": {
            "type": "object",
            "properties": {
                "disable_colors": {
                    "type": "string"
                },
                "format": {
                    "type": "string"
                },
                "format_type": {
                    "type": "string"
                },
                "level": {
                    "type": "string"
                }
            }
        },
        "models.Metadata": {
            "type": "object",
            "properties": {
                "app": {
                    "$ref": "#/definitions/models.AppInfo"
                },
                "cache": {
                    "$ref": "#/definitions/models.CacheInfo"
                },
                "fiber": {
                    "$ref": "#/definitions/models.FiberInfo"
                },
                "files": {
                    "$ref": "#/definitions/models.FilesInfo"
                },
                "headers": {
                    "$ref": "#/definitions/models.HeadersInfo"
                },
                "logging": {
                    "$ref": "#/definitions/models.LoggingInfo"
                },
                "rate_limiter": {
                    "$ref": "#/definitions/models.RateLimiterInfo"
                },
                "server": {
                    "$ref": "#/definitions/models.ServerInfo"
                }
            }
        },
        "models.RateLimiterInfo": {
            "type": "object",
            "properties": {
                "duration": {
                    "type": "string"
                },
                "enabled": {
                    "type": "boolean"
                },
                "max_requests": {
                    "type": "integer"
                }
            }
        },
        "models.ReadinessHealthStatus": {
            "type": "object",
            "properties": {
                "csv": {
                    "type": "string"
                },
                "ready": {
                    "type": "boolean"
                },
                "reason": {
                    "type": "string"
                },
                "redis": {
                    "type": "string"
                }
            }
        },
        "models.ServerInfo": {
            "type": "object",
            "properties": {
                "environment": {
                    "type": "string"
                },
                "port": {
                    "type": "string"
                },
                "timestamp": {
                    "type": "string"
                }
            }
        }
    },
    "securityDefinitions": {
        "BasicAuth": {
            "type": "basic"
        }
    },
    "externalDocs": {
        "description": "OpenAPI",
        "url": "https://swagger.io/resources/open-api/"
    }
}